<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <stroke
                android:width="3dp"
                android:color="@color/colorGrayBorder"
                android:dashWidth="10dp"
                android:dashGap="6dp" />
            <padding
                android:bottom="6dp"
                android:left="8dp"
                android:right="8dp"
                android:top="6dp" />
            <corners android:radius="2dp" />
        </shape>
    </item>
</layer-list>